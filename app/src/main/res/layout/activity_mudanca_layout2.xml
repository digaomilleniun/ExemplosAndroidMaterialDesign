<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_mudanca_layout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="animation.rpires.com.br.exemplosmaterialdesign.activity.transition.MudancaLayoutActivity2">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?attr/actionBarSize"
        android:background="@drawable/toolbar_rounded_corners"
        android:layout_margin="8dp"
        android:elevation="4dp"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

    <View
        android:id="@+id/activity_transition_header"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:transitionName="holder1"
        android:background="?android:colorPrimary" />

    <Button
        android:id="@+id/fab_button"
        android:layout_width="@dimen/fab_size"
        android:layout_height="@dimen/fab_size"
        android:transitionName="fab"
        android:layout_marginTop="-26dp"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:layout_below="@+id/activity_transition_header"
        android:layout_alignParentEnd="true"
        android:elevation="5dp"
        android:background="@drawable/ripple_round"
        android:stateListAnimator="@anim/fab_anim"
        android:onClick="onFabPressed"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_horizontal_margin"
        android:layout_below="@id/activity_transition_header">

    </RelativeLayout>



</RelativeLayout>
